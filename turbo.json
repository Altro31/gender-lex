{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:generate": {
      "cache": true,
      "inputs": [
        "**/*.zmodel"
      ],
      "outputs": []
    },
    "lint": {},
    "test": {},
    "test:e2e": {}
  }
}