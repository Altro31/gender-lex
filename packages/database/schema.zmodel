import "./src/schema/user"
import "./src/schema/chatbot"

datasource db {
    provider = 'postgresql'
    url      = env('DATABASE_URL')
}

plugin ts {
  provider = '@core/typescript'
  output = 'src/generated'
  importWithFileExtension = '.ts'
}

plugin policy {
    provider = "@zenstackhq/plugin-policy"
}
