		"migrate:prod": "prisma migrate deploy"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^6.18.0",
		"@zenstackhq/runtime": "catalog:"
	},
	"devDependencies": {
		"prisma": "6.17.0",
		"@zenstackhq/openapi": "catalog:",
		"zenstack": "catalog:"
	},
	"peerDependencies": {
		"zod": "catalog:",
		"@prisma/client": "catalog:"
	},
	"exports": {
		".": "./index.ts",
		"./models": "./models.ts",
		"./zod": "./zod.ts",
		"./client": "./client.ts",
		"./openapi.yaml": "./openapi.yaml"
	}
}
