{
	"name": "api",
	"version": "0.0.1",
	"description": "GenderLex Backend",
	"author": "Alberto Rodr√≠guez Castro",
	"private": true,
	"license": "MIT",
	"engines": {
		"node": "22.x"
	},
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"generate": "nest start --entryFile generate.js --preserveWatchOutput -p tsconfig.generate.json"
	},
	"dependencies": {
		"@adobe/pdfservices-node-sdk": "^4.1.0",
		"@ai-sdk/google": "^2.0.0-beta.9",
		"@ai-sdk/groq": "^2.0.0-beta.2",
		"@ai-sdk/openai": "^2.0.0-beta.7",
		"@ai-sdk/provider": "^2.0.0-beta.1",
		"@ai-sdk/provider-utils": "^3.0.0-beta.2",
		"@mguay/nestjs-better-auth": "^1.0.2",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/serve-static": "^5.0.3",
		"@repo/auth": "workspace:*",
		"@repo/db": "workspace:*",
		"@zenstackhq/server": "catalog:",
		"adm-zip": "^0.5.16",
		"ai": "^5.0.0-beta.18",
		"axios": "^1.8.4",
		"bcryptjs": "^3.0.2",
		"better-auth": "catalog:",
		"cache-manager": "^6.4.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cors": "^2.8.5",
		"nestjs-cls": "^5.4.2",
		"nestjs-zod": "^4.3.1",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/swagger": "^11.1.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.7",
		"@types/adm-zip": "^0.5.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/multer": "^1.4.12",
		"@types/passport-jwt": "^4.0.1",
		"@types/supertest": "^6.0.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^16.0.0",
		"jest": "^29.7.0",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"prettier": "^3.4.2",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "catalog:",
		"typescript-eslint": "^8.20.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
