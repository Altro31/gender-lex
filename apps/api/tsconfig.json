{
	"extends": "nitro/tsconfig",
	"compilerOptions": {
		"paths": { "@/*": ["./src/*"] },
		"noEmit": false,
		"composite": true,
		"allowImportingTsExtensions": false,
		"declaration": false,
		"plugins": [
			{ "name": "workflow" },
			{ "name": "@effect/language-service" }
		]
	},
	"exclude": ["node_modules"]
}
